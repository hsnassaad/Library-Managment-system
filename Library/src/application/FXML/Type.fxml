<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="Borderpane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.typePage">
   <top>
      <HBox alignment="CENTER" prefHeight="102.0" prefWidth="796.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="rent" contentDisplay="TOP" mnemonicParsing="false" onMouseClicked="#getRent" prefHeight="104.0" prefWidth="463.0" styleClass="img" stylesheets="@../Style/type.css">
               <graphic>
                  <ImageView fx:id="imgRent" fitHeight="77.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/_Rent_Sign-512.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="buy" contentDisplay="TOP" layoutX="292.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#getBuy" prefHeight="87.0" prefWidth="410.0" styleClass="img2" stylesheets="@../Style/type.css">
               <graphic>
                  <ImageView fx:id="imgBuy" fitHeight="69.0" fitWidth="93.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/buy.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </top>
   <center>
      <AnchorPane fx:id="anchor" prefHeight="600.0" prefWidth="796.0" styleClass="anchor" stylesheets="@../Style/type.css" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="texto" fill="#544c35" layoutX="66.0" layoutY="280.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Choose your type..." textAlignment="CENTER" wrappingWidth="664.5366821289062">
               <font>
                  <Font name="System Italic" size="36.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <HBox prefHeight="34.0" prefWidth="796.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backo" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="798.0" text="Back" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
