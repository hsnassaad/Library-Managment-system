               </VBox.margin>
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <TextField fx:id="name" prefHeight="0.0" promptText="Name">
               <VBox.margin>
                  <Insets left="40.0" right="40.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="password" promptText="Password">
               <VBox.margin>
                  <Insets left="40.0" right="40.0" top="20.0" />
               </VBox.margin>
            </PasswordField>
            <HBox prefHeight="10.0" prefWidth="632.0">
