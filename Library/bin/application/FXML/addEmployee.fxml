               </children>
            </HBox>
            <TextField fx:id="salary" layoutX="10.0" layoutY="89.0" prefHeight="0.0" prefWidth="200.0" promptText="Salary">
               <VBox.margin>
                  <Insets left="40.0" right="40.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="42.0" prefWidth="632.0">
               <children>
                  <Label fx:id="label" prefHeight="18.0" prefWidth="174.0" text="Choose your work time:">
                     <HBox.margin>
                        <Insets left="40.0" top="14.0" />
                     </HBox.margin>
                  </Label>
                  <Button fx:id="work" mnemonicParsing="false" onAction="#displayWorkTimes" prefHeight="15.0" prefWidth="42.0" text="+">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="confirm" prefHeight="18.0" prefWidth="273.0">
