                  <Insets left="40.0" top="14.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="53.0" prefWidth="600.0">
               <children>
                  <Button fx:id="save" mnemonicParsing="false" onAction="#save" prefHeight="52.0" prefWidth="126.0" stylesheets="@application.css" text="Save">
                     <HBox.margin>
                        <Insets left="160.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel" prefHeight="52.0" prefWidth="126.0" stylesheets="@application.css" text="Cancel">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
